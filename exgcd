#include <stdio.h>
void exgcd (int a,int b,int *d,int *x,int*y)
{
    if (!b)
    {*x=1;*y=0;*d=a;}
    else 
    {
        exgcd (b,a%b,d,y,x);
        *y-=*x*(a/b);
    }
}
int main(){
    int x,y,gcd;
    int a,b;
    scanf("%d%d",&a,&b);
    exgcd (a,b,&gcd,&x,&y);
    printf("%d %d %d",x,y,gcd);
}
